---
import BrandingLayout from "@/layouts/BrandingLayout.astro";
import { getEntry, getCollection } from "astro:content";
import { EventProgram } from "@/components/event-program";

const events = await getEntry("events", "rouen-france-2024");
const talks = await getCollection("talks");
const speakers = await getCollection("speakers");
const sponsors = await getCollection("sponsors");
---

<BrandingLayout name="Conference Card">
  <EventProgram
    event={events.data}
    talks={talks}
    speakers={speakers}
    sponsors={sponsors}
    client:load
  />
</BrandingLayout>
