---
import MainLayout from "@/layouts/MainLayout.astro";
import SEO from "@/components/SEO/index.astro";
import MainPageTitle from "@/components/MainPageTitle/index.astro";
import bgImage from "@/assets/images/events.jpeg";
import { AssetsContent } from "@/components/BrandAssets/AssetsContent";
---

<MainLayout>
  <SEO
    slot="seo"
    title="Fork it! Community brand assets"
    description="Find and download all Fork it! Community logo"
  />
  <MainPageTitle
    title="Fork it! Community brand assets"
    subtitle="Find and download all Fork it! Community logo"
    imageSrc={bgImage}
  />
  <section class="m-auto max-w-7xl px-4 py-16">
    <AssetsContent />
  </section>
</MainLayout>
